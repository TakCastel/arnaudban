{
  "name": "arnaudban-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "accessibility": "pa11y http://localhost:3000",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "performance": "npm run build && npm run start & sleep 5 && npm run lighthouse && pkill -f 'next start'",
    "seo-check": "npm run build && npm run start & sleep 5 && curl -s http://localhost:3000/sitemap.xml && curl -s http://localhost:3000/robots.txt && pkill -f 'next start'"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "pa11y": "^6.2.3",
    "lighthouse": "^11.6.0"
  }
}
